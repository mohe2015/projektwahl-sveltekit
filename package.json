{
	"name": "projektwahl-sveltekit",
	"version": "0.0.1",
	"license": "AGPL-3.0-or-later",
	"author": {
		"name": "Moritz Hedtke",
		"email": "Moritz.Hedtke@t-online.de"
	},
	"bugs": {
		"url": "https://github.com/mohe2015/projektwahl-sveltekit/issues"
	},
	"homepage": "https://github.com/mohe2015/projektwahl-sveltekit",
	"repository": {
		"type": "git",
		"url": "https://github.com/mohe2015/projektwahl-sveltekit.git"
	},
	"description": "Software to manage choosing projects and assigning people to projects.",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. .",
		"prepare": "husky install",
		"test": "jest src",
		"update-snapshot": "jest --updateSnapshot",
		"test:watch": "npm run test -- --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.0.0-next.29",
		"@sveltejs/kit": "^1.0.0-next.119",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/svelte": "^3.0.3",
		"@types/bootstrap": "^5.1.2",
		"@types/jest": "^27.0.1",
		"@typescript-eslint/eslint-plugin": "^4.29.3",
		"@typescript-eslint/parser": "^4.29.3",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"husky": "^7.0.2",
		"jest": "^26.6.3",
		"lint-staged": "^11.1.2",
		"prettier": "~2.3.2",
		"prettier-plugin-svelte": "^2.4.0",
		"svelte": "^3.42.4",
		"svelte-jester": "^1.8.2",
		"svelte-preprocess": "^4.8.0",
		"ts-jest": "^26.5.6",
		"tslib": "^2.3.1",
		"typescript": "^4.4.2"
	},
	"type": "module",
	"dependencies": {
		"@types/node-fetch": "^2.5.12",
		"argon2": "^0.28.2",
		"bootstrap": "^5.1.0",
		"csv-parse": "^4.16.2",
		"esbuild-linux-64": "^0.12.24",
		"node-fetch": "^2.6.1",
		"postgres": "^2.0.0-beta.5",
		"sveltestrap": "^5.6.2"
	},
	"lint-staged": {
		"*.{ts,svelte}": "eslint --cache --fix",
		"*.{ts,svelte,css,md}": "prettier --write"
	},
	"jest": {
		"transform": {
			"^.+\\.svelte$": "svelte-jester",
			"^.+\\.ts$": "ts-jest"
		},
		"moduleFileExtensions": [
			"js",
			"svelte",
			"ts"
		]
	}
}
